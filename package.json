{
    "name": "flow-lang",
    "version": "0.1.0",
    "description": "A domain-specific language for AI agent and workflow orchestration",
    "type": "module",
    "main": "dist/cli/index.js",
    "bin": {
        "flow": "dist/cli/index.js"
    },
    "files": [
        "dist/",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "tsc",
        "test": "vitest run",
        "test:watch": "vitest",
        "lint": "eslint src/ tests/",
        "check": "tsc --noEmit",
        "prepublishOnly": "npm run build && npm run test",
        "docs:dev": "vitepress dev docs",
        "docs:build": "vitepress build docs",
        "docs:preview": "vitepress preview docs"
    },
    "keywords": [
        "flow",
        "dsl",
        "workflow",
        "orchestration",
        "ai-agent",
        "ai",
        "automation",
        "no-code",
        "low-code"
    ],
    "author": "Abraham Oluwa",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AbrahamOluwa/flow-lang.git"
    },
    "homepage": "https://github.com/AbrahamOluwa/flow-lang",
    "engines": {
        "node": ">=18.0.0"
    },
    "devDependencies": {
        "@types/express": "^5.0.6",
        "@types/node": "^22.0.0",
        "@types/supertest": "^6.0.3",
        "@typescript-eslint/eslint-plugin": "^8.0.0",
        "@typescript-eslint/parser": "^8.0.0",
        "eslint": "^9.0.0",
        "supertest": "^7.2.2",
        "typescript": "^5.6.0",
        "vitepress": "^1.6.4",
        "vitest": "^2.0.0"
    },
    "dependencies": {
        "@anthropic-ai/sdk": "^0.73.0",
        "chalk": "^5.3.0",
        "commander": "^12.0.0",
        "dotenv": "^17.2.4",
        "express": "^5.2.1",
        "openai": "^6.18.0"
    }
}
